<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*@Styles.Render("~/Content/css")
        @Styles.Render("~/Content/themes/base/css")
        @Scripts.Render("~/bundles/modernizr")
        @RenderSection("styles", required: false)*@

    @Styles.Render("~/bundles/cssdatatables")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)

    <style>
        .affix {
            top: 0;
            width: 100%;
            z-index: 9999 !important;
        }

            .affix + .container-fluid {
                padding-top: 70px;
            }
    </style>
</head>

<body id="myPage">
    <div class="container-fluid" style="background-color: #66CDE1; color: #fff; height: 60px; opacity:0.9;">
        <img src="data:text/plain;base64," height="60" width="60" class="img-responsive center-block">

    </div>

    <nav class="navbar navbar-inverse navbar-static-top" data-spy="affix" data-offset-top="80">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("HR", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @if (ViewContext.HttpContext.User.IsInRole("Admin"))
                    {
                        <li>@Html.ActionLink("Employee", "Index", "Employee")</li>
                    }
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">Market Value <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Certification", "Index", "Certification")</li>
                            <li>@Html.ActionLink("Experience", "Index", "Experience")</li>
                            <li>@Html.ActionLink("Network", "Index", "Network")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#"> Premium <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Performance", "Index", "Performance")</li>
                            <li>@Html.ActionLink("Working Hours", "WorkingHoursIndex", "Attendance")</li>
                            <li>@Html.ActionLink("Achievement", "Index", "Achievement")</li>
                            <li>@Html.ActionLink("Communication Skill", "Index", "CommunicationSkill")</li>
                        </ul>
                    </li>
                    @if (ViewContext.HttpContext.User.IsInRole("Admin"))
                    {
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#"> Report <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Report", "GetEmployees", "Report")</li>
                                <li>@Html.ActionLink("Charts", "Index", "Charts")</li>
                            </ul>
                        </li>
                    }
                    @if (ViewContext.HttpContext.User.IsInRole("CEO"))
                    {
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#"> Report <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Report", "GetEmployees", "Report")</li>
                                <li>@Html.ActionLink("Charts", "Index", "Charts")</li>
                            </ul>
                        </li>                    }
                    <li>@Html.ActionLink("Attendance", "Index", "Attendance")</li>
                    <li>@Html.ActionLink("Evaluation", "Index", "Evaluation")</li>
                    <li>@Html.ActionLink("Salary", "Index", "Salary")</li>

                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </nav>

    <div class="container body-content ">
        @RenderBody()
        <hr />
    </div>
    <footer class="panel-footer text-center">
        <a href="#myPage" title="To Top">
            <span class="glyphicon glyphicon-chevron-up"></span>
        </a>
        <h3 class="text-center">CONTACT</h3>
        <div class="row">
            <div class="col-sm-12">
                <p><span class="glyphicon glyphicon-map-marker"></span> Jeddah, SA</p>
                <p><span class="glyphicon glyphicon-phone"></span> +00 1515151515</p>
                <p><span class="glyphicon glyphicon-envelope"></span> myemail@something.com</p>
            </div>
        </div>
    </footer>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/jqueryui")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jsdatatables")
    @RenderSection("scripts", required: false)



    <script>
        $(document).ready(function () {
            // Add smooth scrolling to all links in navbar + footer link
            $(".navbar a, footer a[href='#myPage']").on('click', function (event) {
                if (this.hash !== "") {
                    event.preventDefault();

                    // Store hash
                    var hash = this.hash;
                    $('html, body').animate({
                        scrollTop: $(hash).offset().top
                    }, 900, function () {

                        window.location.hash = hash;
                    });
                }
            });

            $(window).scroll(function () {
                $(".slideanim").each(function () {
                    var pos = $(this).offset().top;

                    var winTop = $(window).scrollTop();
                    if (pos < winTop + 600) {
                        $(this).addClass("slide");
                    }
                });
            });
        })
    </script>

</body>
</html>
